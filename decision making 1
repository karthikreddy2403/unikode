#sece dining

m=input()
r=int(input())
if m=='front':
    if r==1:
        print("Left Handed")
    elif r==2:
        print("Right Handed")
    else:
        print("Invalid Input")
elif m=="rear":
    if r==1:
         print("Right Handed")
    elif r==2:
        print("Left Handed")
    else:
        print("Invalid Input")
else:
    print("Invalid Input")
        
        
        #intersection of circles
        
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
k=(pow(x1-x2,2)+pow(y1-y2,2))
d=pow(r1+r2,2)
if(k>d):
    print("The Circles do not overlap ")
elif(k<d):
    print("The Circles overlap")
else:
    print("The Circles are tangential ")
    
    #microwave owen
    
    x=int(input("Enter the number of items\n"))
y=float(input("Enter the single item heating time\n"))
if(x==1):
    print("The recommended heating time is ",y)
elif(x==2):
     print("The recommended heating time is %.2f"%(1.5*y))
elif(x==3):
    print("The recommended heating time is ",2*y)
else:
    print("Number of items is more")
    
    
    
    
    
    
