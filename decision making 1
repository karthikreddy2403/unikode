#grading system

m=int(input())
if(m==100):
    print("S")
elif(m>=90 and m<100):
    print("A")
elif(m>=80 and m<90):
    print("B")
elif(m>=70 and m<80):
    print("C")
elif(m>=60 and m<70):
    print("D")
elif(m>=50 and m<60):
    print("E")
elif(0<=m<50):
    print("F")
else:
    print("Invalid Input")
    
    #profit or loss
    
    x= float(input())
y= float(input())
z=y*12
if(z>x):
    print("Enter the price of a dozen mangoes\nEnter the price at which 1 mango is being sold\nProfit: Rs.%.2f"%(z-x))
elif(x>z):
    print("Enter the price of a dozen mangoes\nEnter the price at which 1 mango is being sold\nLoss: Rs.%.2f"%(x-z))
else:
    print("Enter the price of a dozen mangoes\nEnter the price at which 1 mango is being sold\nNo profit nor loss")
    
    #fee collection
    
    s=input()
a="MSDS"
b="MSH"
c="MGSDS"
d="MGSH"


tf=float(input("Enter the student type\n"))
bf=float(input("Enter tuition fee\n"))
hf=float(input("Enter hostel fee\n"))

if(s =="a"):
    fee=tf+bf
    print("The fees to be paid by the student is Rs.%.2f"%fee)
elif(s=="b"):
    fee=tf+hf
    print("The fees to be paid by the student is Rs.%.2f"%fee)
elif(s=="c"):
    fee=tf*(1.5)+bf
    print("The fees to be paid by the student is Rs.%.2f"%fee)
elif(s=="d"):
    fee=tf*(1.5)+hf
    print("The fees to be paid by the student is Rs.%.2f"%fee)
else:
    print("invalid input")
    
    #finding age
    
    a=int(input("Enter Year of Birth\n"))
b=int(input("Enter Current year\n"))
if(b>a):
    print("Your age is ",b-a)
else:
    print("Your age is ",((100-a)+b))

#lab allocation

L1=int(input("Enter x\n"))
L2=int(input("Enter y\n"))
L3=int(input("Enter z\n"))
if(L1<L2 and L1<L3):
    print("L1 has the minimal seating capacity")
elif(L2<L1 and L2<L3):
    print("L2 has the minimal seating capacity")
else:
    print("L3 has the minimal seating capacity")
    


